{
  "dbSettings": {
    "host": "localhost",
    "port": 5420,
    "databaseName": "IdentityDb",
    "userId": "admin",
    "password": "admin1234"
  },
  "identitySettings": {
    "password": {
      "requireDigit": true,
      "requireLowercase": true,
      "requireUppercase": false,
      "requireNonAlphanumeric": false,
      "requiredUniqueChars": 0
    },
    "email": {
      "requireConfirmation": true,
      "requiredUniqueEmail": false
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "elasticConfiguration": {
    "uri": "http://localhost:9200"
  }
}
